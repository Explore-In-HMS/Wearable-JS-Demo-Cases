<div class="container" onswipe="touchMove">
    <div class="content">
        <text class="title">
            Brightness value : {{ brightnessValue }}
        </text>
        <input id="input" class="input" type="text" value=""  enterkeytype="send"
               placeholder="Brightness" onchange="onTextChange">
        </input>
        <input class="button" type="button" value="Set" onclick="buttonClick"></input>
    </div>
</div>
